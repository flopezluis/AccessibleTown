    function loadMap(data) {
        maps = data;
        maps.setModeAdding(true);
        }
    function add_point() {
        $.fancybox($("#my_form"),{
            'scrolling'		: 'no',
            'titleShow'		: false,
        });

        $('#my_form').ajaxForm(function() { 
            alert("Thank you for your comment!"); 
        }); 
        
    
    }
{% endblock %}

{% block jquery_init %}
        Maps(loadMap, add_point);
{% endblock %}

